/* pages/invote/invote.wxss */
page {
    height: 100%;
    width: 100%;
}
.container {
    width: 100%;
    height: 100%;
}
.searchInput {
    width: 750rpx;
    height: 120rpx;
    /* position: absolute; */
    display: flex;
    align-items: center;
    position: fixed;
    background-color: #fff;
}
/* .searchInput::after {
    content: '';
    display: block;
    overflow: hidden;
    clear: both;
} */
.qrc {
    flex:1;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 20rpx;
}
.input {
    flex:7;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 20rpx;
    border: 1px solid #ffffff;
    background-color: #f3f6f6;
    color:#b5b8b8;
    font-size: 30rpx;
    width: 500rpx;
    border-radius: 50rpx;
    opacity: 0.7;
}
.input image {
    flex: 0.2;
    display: flex;
    justify-content: center;
    align-items: center;
}
.input input {
    flex: 2;
    display: flex;
    justify-content: center;
    align-items: center;
}
.erji {
    flex:1;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 20rpx;
}
.xf {
    flex:1;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 15rpx;
}
.iconList {
    width: 750rpx;
    height: 400rpx;
    float: left;
    margin-top: 120rpx;
}
.iconList::after {
    content: '';
    display: block;
    overflow: hidden;
    clear: both;
}
.swiper-box {
    height: 100%;
    width: 100%;
    float: left;
}
.icons {
    width: 25%;
    height: 160rpx;
    float: left;
    align-items: center;
}
.iconImage {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.iconImage image {
    display: flex;
    justify-content: center;
    align-items: center;
}
.iconImage text {
    line-height: 100rpx;
    font-size: 32rpx;
    text-align: center;
}
.ximg {
    height: 200rpx;
    width: 750rpx;
    position: absolute;
}
.goImg {
    width: 70rpx;
    height: 70rpx;
    position: absolute;
    margin-top: 80rpx;
    margin-left: 620rpx;
    animation: scaref 1.5s linear infinite;
}
@keyframes scaref {
    0%{
        opacity: 1;
        transform: scale(2);
      } 
      20%{
        opacity: 1;
        transform: scale(1.5);
      }
      89%,100%{
        opacity: 1;
        transform: scale(1);
      }
}
.goImg image {
    width: 100%;
    height: 100%;
}
.twoImg {
    overflow: hidden;
    position: relative;
    width: 750rpx;
    height: 220rpx;
}
.title-text {
    font-weight: bold;
    font-size: 38rpx;
    height: 80rpx;
    width: 100%;
    text-align: left;
    margin-left: 20rpx;
}
.two-models {
    width: 100%;
    height: 270rpx;
    display: flex;
}
.one-model {
    width: 40%;
    height: 80%;
    margin-left: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    border: 1rpx solid #fff;
    box-shadow: 2px 0 3px -1px #b5b8b8;
}
.img-title {
    font-size: 32rpx;
    font-weight: bold;
    flex: 2;
    width: 100%;
    align-items: center;
    justify-content: center;
    display: block;
    text-align: center;
}
.img-title image {
    align-items: center;
    vertical-align: middle;
    justify-content: center;
}
.img-title text {
    vertical-align: middle;
    align-items: center;
    justify-content: center;
    margin-left: 10rpx;
}
.span-text {
    flex: 1;
    width: 100%;
    font-size: 20rpx;
    font-weight: 400;
    text-align: center;
    align-items: center;
    justify-content: center;
    display: block;
}
.persent {
    flex: 2.6;
    width: 100%;
    text-align: center;
    font-size: 40rpx;
    align-items: center;
    justify-content: center;
    display: block;
    line-height: 47.27px;
    color: red;
}
.span1-text {
    flex: 1;
    width: 100%;
    text-align: center;
    font-size: 18rpx;
    font-weight: 200;
    align-items: center;
    justify-content: center;
    display: block;
}
.two-model {
    width: 40%;
    height: 80%;
    margin-left: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    border: 1rpx solid #fff;
    box-shadow: -2px 0 3px -1px #b5b8b8;
}
.fixed-deposit {
  height: 350rpx;
  /* margin-top: 20rpx; */
  padding-left: 20rpx;
  width: 100%;
}
.fixed {
  font-weight: bold;
  font-size: 38rpx;
  height: 80rpx;
  width: 100%;
  text-align: left;
  line-height: 80rpx;
}
scroll-view {
  width:100%;
  overflow:hidden;
  white-space:nowrap;
}
.a {
  display:inline-block;
  align-items: center;
  justify-content: center;
  text-align: center;
  width: 40%;
  height: 100%;
  padding: 20rpx;
  overflow: hidden;
  box-sizing: border-box;
  border-right: 1px solid #b5b8b8;
  /* white-space: nowrap; */
}
/* .f {
  border-right: 1px solid #b5b8b8; 
} */
.b {
  font-size: 26rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  white-space:normal;
}
.c {
  color: red;
  text-align: center;
  font-size: 40rpx;
  align-items: center;
  justify-content: center;
